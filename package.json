{
  "name": "might-cli",
  "version": "2.3.2",
  "description": "A no-code solution for performing end-to-end tests",
  "scripts": {
    "prepare": "npm run build",
    "build": "rm -rf build && mkdir build && npx tsc --removeComments -p tsconfig.json",
    "watch": "npx tsc -w -p tsconfig.json"
  },
  "engines": {
    "node": ">=12"
  },
  "bin": {
    "might": "build/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ker0olos/might-cli.git"
  },
  "author": "Kerolos Zaki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ker0olos/might-cli/issues"
  },
  "homepage": "https://github.com/ker0olos/might-cli#readme",
  "keywords": [
    "testing",
    "coverage",
    "coverage-reports",
    "end-to-end-testing",
    "ui-testing",
    "front-end-testing",
    "end-to-end-test-automation",
    "end-to-end-testing-interface"
  ],
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "convert-source-map": "^1.8.0",
    "draftlog": "1.0.12",
    "exit": "^0.1.2",
    "fs-extra": "^10.0.0",
    "is-ci": "^3.0.0",
    "istanbul-lib-coverage": "^3.0.0",
    "istanbul-lib-report": "^3.0.0",
    "istanbul-reports": "^3.0.2",
    "jimp": "^0.16.1",
    "looks-same": "^7.3.0",
    "md5": "^2.3.0",
    "might-core": "^1.0.2",
    "minimist": "^1.2.5",
    "nanomatch": "^1.2.13",
    "node-fetch": "^2.6.1",
    "p-limit": "^3.1.0",
    "playwright": "^1.12.3",
    "prompts": "^2.4.1",
    "ps-tree": "^1.2.0",
    "sanitize-filename": "^1.6.3",
    "source-map": "^0.7.3",
    "v8-to-istanbul": "^8.0.0"
  },
  "devDependencies": {
    "@types/convert-source-map": "^1.5.2",
    "@types/exit": "^0.1.31",
    "@types/fs-extra": "^9.0.12",
    "@types/is-ci": "^3.0.0",
    "@types/istanbul-lib-coverage": "^2.0.3",
    "@types/istanbul-lib-report": "^3.0.0",
    "@types/md5": "^2.3.1",
    "@types/minimist": "^1.2.2",
    "@types/node-fetch": "^2.5.11",
    "@types/prompts": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-plugin-security": "^1.4.0",
    "typescript": "^4.3.5"
  }
}
